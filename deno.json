{
  "lint": {
    "include": [
      "deploy.json",
      "src/",
      "cli/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "ban-unused-ignore"
      ]
    }
  },
  "fmt": {
    "include": [
      "deploy.json",
      "src/",
      "cli/",
      "build.ts"
    ],
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 2,
    "singleQuote": true
  },
  "imports": {
    "@alicloud/openapi-client": "npm:@alicloud/openapi-client@^0.4.12",
    "@alicloud/sls20201230": "npm:@alicloud/sls20201230@^5.4.1",
    "@alicloud/tea-typescript": "npm:@alicloud/tea-typescript@^1.8.0",
    "@alicloud/tea-util": "npm:@alicloud/tea-util@^1.4.9",
    "@std/assert": "jsr:@std/assert@1",
    "@std/async": "jsr:@std/async@^1.0.7"
  },
  "nodeModulesDir": "auto"
}
