{
  "name": "@zsqk/aliyun-sdk",
  "version": "0.1.2",
  "license": "MIT",
  "exports": {
    "./cli/oss-before-upload": "./cli/oss-before-upload.ts",
    "./oss/get-meta": "./src/oss/get-meta.ts",
    "./oss/upload": "./src/oss/upload.ts",
    "./sls/get-logs": "./src/sls/get-logs.ts",
    "./sls/for-fc": "./src/sls/for-fc.ts"
  },
  "lint": {
    "include": [
      "deno.json",
      "src/",
      "cli/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "ban-unused-ignore"
      ]
    }
  },
  "fmt": {
    "include": [
      "deno.json",
      "src/",
      "cli/",
      "build.ts"
    ],
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 2,
    "singleQuote": true
  },
  "unstable": ["detect-cjs"],
  "imports": {
    "@alicloud/openapi-client": "npm:@alicloud/openapi-client@^0.4.15",
    "@alicloud/oss20190517": "npm:@alicloud/oss20190517@^1.0.6",
    "@alicloud/sls20201230": "npm:@alicloud/sls20201230@^5.9.0",
    "@alicloud/tea-typescript": "npm:@alicloud/tea-typescript@^1.8.0",
    "@alicloud/tea-util": "npm:@alicloud/tea-util@^1.4.10",
    "@std/assert": "jsr:@std/assert@^1.0.15",
    "@std/async": "jsr:@std/async@^1.0.14",
    "@std/cli": "jsr:@std/cli@^1.0.23",
    "@zsqk/crc64": "jsr:@zsqk/crc64@^0.3.3",
    "zod": "npm:zod@^3.23.8"
  },
  "compilerOptions": {
    "lib": ["deno.ns", "esnext", "dom"],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "strict": true
  },
  "nodeModulesDir": "auto"
}
